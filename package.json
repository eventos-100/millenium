{
  "name": "millenium-automotive",
  "version": "1.0.0",
  "main": "src/index.ts",
  "type": "module",
  "description": "Premium automotive dealership platform with integrated Webflow Cloud storage, CRM, and analytics",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "types": "wrangler types",
    "d1:migrate": "wrangler d1 migrations apply millenium_db",
    "d1:migrate:local": "wrangler d1 migrations apply millenium_db --local",
    "d1:console": "wrangler d1 execute millenium_db",
    "d1:console:local": "wrangler d1 execute millenium_db --local",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'",
    "validate": "./validate.sh",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "webflow",
    "cloudflare",
    "workers",
    "automotive",
    "luxury",
    "crm",
    "millenium",
    "premium"
  ],
  "author": "Millenium Automotive Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eventos-100/millenium.git"
  },
  "dependencies": {
    "@webflow/webflow-cli": "^1.8.30"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240729.0",
    "@cloudflare/vitest-pool-workers": "^0.4.5",
    "@types/node": "^20.14.8",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "vitest": "1.5.0",
    "wrangler": "^3.60.3",
    "vite": "^5.3.1"
  },
  "peerDependencies": {
    "@opennextjs/cloudflare": "^1.0.0"
  }
}